{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:event:allow-emit",
    "core:event:allow-listen",
    "core:event:allow-emit-to",
    "core:event:allow-unlisten",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-load",
    "sql:allow-select",
    "window-state:default"
  ]
}
